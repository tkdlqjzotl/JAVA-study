요따구로 출력



	int k=1;
		for(int i=0;i<5;i++)
		{
			
			for(int j=0;j<i;j++)
			{
				System.out.print("    ");
				
				
			}
			
			
			
			for(int j=0;j<5-i;j++)
			{
				System.out.print(k);
				if(i>=2)System.out.print("  ");
				else System.out.print("   ");				
				k++;
				
			}
			System.out.println();
			
			
		}
